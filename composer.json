{
  "name": "sandronimus/nelmio-form-get-parameter-plugin-bundle",
  "description": "Extends NelmioApiDocBundle by providing ability to auto-register simple forms items as GET parameters.",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "nelmio/api-doc-bundle": "^4.0",
    "symfony/framework-bundle": "^6.1",
    "symfony/form": "^6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Sandronimus\\NelmioFormGetParameterPluginBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sandronimus\\NelmioFormGetParameterPluginBundle\\Tests\\": "Tests/"
    }
  }
}
