{
  "name": "sandronimus/nelmio-form-get-parameter-plugin-bundle",
  "description": "Extends NelmioApiDocBundle by providing ability to auto-register simple forms items as GET parameters.",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "nelmio/api-doc-bundle": "^4.0",
    "symfony/framework-bundle": "^6.1|^7.1",
    "symfony/form": "^6.1|^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/asset": "^6.4|^7.1",
    "symfony/debug-bundle": "^6.4|^7.1",
    "symfony/flex": "^2.4",
    "symfony/runtime": "^6.4|^7.1",
    "symfony/twig-bundle": "^6.4|^7.1",
    "symfony/web-profiler-bundle": "^6.4|^7.1",
    "twig/twig": "^3"
  },
  "autoload": {
    "psr-4": {
      "Sandronimus\\NelmioFormGetParameterPluginBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sandronimus\\NelmioFormGetParameterPluginBundle\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*",
      "docker": false
    },
    "public-dir": "/Tests/public"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "symfony/flex": true
    }
  }
}
